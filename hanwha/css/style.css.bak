}

div.button1 {

	display: none;
	width: 68px;
	height: 24px;
	background: #f37321;
	border-radius: 5px;
	text-align: center;
	position:absolute;
	top: 28px; right: 89px;
	z-index:10;
}

div.button2 {

	display: none;
	width: 68px;
	height: 24px;
	background: #f37321;
	border-radius: 5px;
	text-align: center;
