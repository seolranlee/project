/*** top-bar button ****/

div.button1 p, div.button2 p, div.button3 p {
	
	color: white;
	font-size: 11px;
	line-height: 24px;
}

div.button1 {

	display: none;
	width: 68px;
	height: 24px;
	background: #f37321;
	border-radius: 5px;
	text-align: center;
	position:absolute;
	top: 28px; right: 89px;
	z-index:10;
}

div.button2 {

	display: none;
	width: 68px;
	height: 24px;
	background: #f37321;
	border-radius: 5px;
	text-align: center;
	position:absolute;
	top: 28px; right: 40px;
	z-index:10;
}

div.button3 {

	display: none;
	width: 35px;
	height: 24px;
